{
  "name": "@turbostarter/ui-mobile",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./icons": "./src/lib/icons/index.ts",
    "./*": "./src/components/*.tsx"
  },
  "scripts": {
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "format": "prettier --check . --ignore-path ../../../.gitignore",
    "lint": "eslint",
    "typecheck": "tsc --noEmit",
    "ui:add": "pnpm dlx @react-native-reusables/cli@latest add && prettier src --write --list-different"
  },
  "prettier": "@turbostarter/prettier-config",
  "dependencies": {
    "@gorhom/bottom-sheet": "5.1.1",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@rn-primitives/avatar": "1.1.0",
    "@rn-primitives/checkbox": "1.1.0",
    "@rn-primitives/context-menu": "1.1.0",
    "@rn-primitives/label": "1.1.0",
    "@rn-primitives/radio-group": "1.1.0",
    "@rn-primitives/select": "1.1.0",
    "@rn-primitives/slot": "1.1.0",
    "@rn-primitives/switch": "1.1.0",
    "@rn-primitives/tabs": "1.1.0",
    "@rn-primitives/types": "1.1.0",
    "@turbostarter/i18n": "workspace:*",
    "@turbostarter/shared": "workspace:*",
    "@turbostarter/ui": "workspace:*",
    "lucide-react-native": "0.439.0",
    "nativewind": "~4.0.36",
    "react-native": "catalog:",
    "react-native-svg": "15.2.0",
    "zustand": "5.0.2"
  },
  "devDependencies": {
    "@turbostarter/eslint-config": "workspace:*",
    "@turbostarter/prettier-config": "workspace:*",
    "@turbostarter/tsconfig": "workspace:*",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "react": "catalog:react19",
    "typescript": "catalog:",
    "zod": "catalog:"
  }
}
