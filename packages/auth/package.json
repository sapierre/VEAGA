{
  "name": "@turbostarter/auth",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./src/index.ts"
    },
    "./env": {
      "types": "./dist/env/index.d.ts",
      "default": "./src/env/index.ts"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "default": "./src/server.ts"
    },
    "./client/*": {
      "types": "./dist/client/*.d.ts",
      "default": "./src/client/*.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "db:generate": "SKIP_ENV_VALIDATION=1 pnpm dlx @better-auth/cli@latest generate --config src/server.ts --output ../db/src/schema/auth/index.ts --y",
    "dev": "tsc",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@turbostarter/eslint-config": "workspace:*",
    "@turbostarter/prettier-config": "workspace:*",
    "@turbostarter/tsconfig": "workspace:*",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "typescript": "catalog:"
  },
  "prettier": "@turbostarter/prettier-config",
  "dependencies": {
    "@better-auth/expo": "1.1.14",
    "@t3-oss/env-core": "0.11.1",
    "@turbostarter/db": "workspace:*",
    "@turbostarter/email": "workspace:*",
    "@turbostarter/shared": "workspace:*",
    "better-auth": "1.1.14",
    "standardwebhooks": "1.0.0",
    "zod": "catalog:"
  }
}
